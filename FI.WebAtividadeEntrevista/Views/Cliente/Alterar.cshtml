@model WebAtividadeEntrevista.Models.ClienteModel
@{
    ViewBag.Title = "Alterar Cliente";
}
@section scripts{
    <script>
        var urlPost = '@Url.Action("Alterar", "Cliente", new { area = "" })';
        var obj = @Html.Raw(Json.Encode(Model));
        var urlRetorno = '@Url.Action("Index", "Cliente", new { area = "" })';

        var urlPostIncluirBenefeciario = '@Url.Action("IncluirBeneficiario", "Cliente", new { area = "" })';
        var urlPostAlterarBenefeciario = '@Url.Action("AlterarBeneficiario", "Cliente", new { area = "" })';
        var urlExcluirBenefeciario = '@Url.Action("ExcluirBeneficiario", "Cliente", new { area = "" })';

        var urlBeneficiarioList = '@Url.Action("BeneficiarioList", "Cliente", new { id = @Model.Id, area = "" })';
    </script>
    @Scripts.Render("~/bundles/altClientes")
    @Scripts.Render("~/bundles/listBeneficiarios")
    @Scripts.Render("~/bundles/validacao.js")
}
<h2>@Html.Raw(ViewBag.Title)</h2>
@Html.Partial("Forms")